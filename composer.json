{
    "name": "randomstate/doctrine-scopes",
    "description": "Global query scopes for Doctrine2",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Connor Forsyth",
            "email": "connor@randomstate.co.uk"
        }
    ],
    "autoload": {
      "psr-4": {
        "RandomState\\DoctrineScopes\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "RandomState\\DoctrineScopes\\Tests\\": "tests",
        "Tests\\": "vendor/laravel/laravel/tests"
      },
      "classmap": [
        "vendor/laravel/laravel/tests/TestCase.php"
      ]
    },
    "require": {
        "doctrine/orm": "^2.6"
    },
    "require-dev": {
        "laravel/laravel": "^5.7",
        "laravel-doctrine/orm": "^1.4",
        "phpunit/phpunit": "^7.0"
    }
}
